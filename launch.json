{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PyMARL2: Run QMIX with SC2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "args": [
                "--config=qmix",
                "--env-config=sc2",
                "with",
                "env_args.map_name=corridor"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}
